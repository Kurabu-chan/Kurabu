{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch via npm",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run-script", "dev", "-w=@kurabu/api"]
		},
		{
			"name": "Attach to @kurabu/app",
			"request": "attach",
			"type": "reactnative",
			"cwd": "${workspaceFolder}/apps/app",
			"port": 19000
		}
	]
}
