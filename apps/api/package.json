{
    "name": "@kurabu/api",
    "version": "2.0.0",
    "description": "",
    "main": "build/start.js",
    "scripts": {
        "format": "prettier --write .",
        "test-setup": "yarn install chai mocha ts-node @types/chai @types/mocha",
        "test": "mocha \"tests/**/*.ts\" --config .mocharc.json --timeout 15000",
        "dev": "nodemon --config \"./util/nodemon.json\"/",
        "build": "yarn rimraf ./build/ && yarn tsc",
        "start": "yarn cross-env TS_NODE_PROJECT=tsconfig.json node -r tsconfig-paths/register -r ts-node/register build/start.js",
        "db:addmigration": "ts-node -r tsconfig-paths/register ./src/database/createmigration.ts",
        "db:localmigration": "ts-node -r tsconfig-paths/register ./src/database/runMigration.ts",
        "db:downconfig": "ts-node -r tsconfig-paths/register ./src/database/downgradeconfig",
        "db:listmigrations": "ts-node -r tsconfig-paths/register ./src/database/listmigrations.ts",
        "lint": "eslint \"**/*.ts\"",
        "install:dev": "yarn install --only=dev",
        "install:prod": "yarn install --only=prod",
        "test:coverage": "tsc && nyc yarn run test",
        "lint:fix": "eslint \"**/*.ts\" --fix",
        "build:docker": "mkdir -p ./docker/build/ && docker build -t kurabu/api:0.1 --output type=tar,dest=./docker/build/build.tar -f ./docker/Dockerfile ../../ && docker import ./docker/build/build.tar kurabu/api",
        "check-build-required": "echo \"@kurabu/api\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@overnightjs/core": "^1.7.5",
        "@overnightjs/logger": "^1.2.0",
        "@sendgrid/mail": "^7.4.2",
        "@types/deep-diff": "^1.0.1",
        "@types/express": "^4.17.11",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^14.14.37",
        "@types/node-fetch": "^2.5.8",
        "@types/pg": "^7.14.10",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/umzug": "^2.3.2",
        "@types/uuid": "^8.3.4",
        "@types/validator": "^13.1.3",
        "body-parser": "^1.19.0",
        "crypto": "^1.0.1",
        "deep-diff": "^1.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "node-fetch": "^2.6.6",
        "path": "^0.12.7",
        "pg": "^8.5.1",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.9.0",
        "sequelize-typescript": "^2.1.1",
        "sequelize-typescript-migration-rafaeltab": "^0.0.1-beta.9",
        "source-map-support": "^0.5.20",
        "swagger-ui-express": "^4.3.0",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.10.1",
        "tslib": "^2.1.0",
        "tslint-to-eslint-config": "^2.9.1",
        "tsyringe": "^4.4.0",
        "umzug": "^3.0.0",
        "uuid": "^8.3.2",
        "@kurabu/env-schema": "*"
    },
    "devDependencies": {
        "@kurabu/config": "*",
        "@kurabu/tsconfig": "*",
        "@types/bluebird": "^3.5.33",
        "@types/chai": "^4.2.15",
        "@types/mocha": "^8.2.1",
        "@typescript-eslint/eslint-plugin": "^4.28.4",
        "@typescript-eslint/eslint-plugin-tslint": "^4.28.4",
        "@typescript-eslint/parser": "^4.28.4",
        "chai": "^4.3.0",
        "eslint": "^7.31.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsdoc": "^35.5.1",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "mocha": "^8.3.0",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "sequelize-cli": "^6.2.0",
        "sqlite3": "^5.0.0",
        "ts-mockito": "^2.6.1",
        "typescript": "^4.4.4"
    }
}