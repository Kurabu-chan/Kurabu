{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "data": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "node": {
                        "$ref": "./details.schema.json"
                    },
                    "ranking": {
                        "type": "object",
                        "properties": {
                            "rank": {
                                "type": "integer"
                            }
                        }

                    }
                },
                "required": [
                    "node", "ranking"
                ]
            }
        },
        "paging": {
            "type": "object",
            "properties": {
                "previous": {
                    "type": "string",
                    "format": "uri"
                },
                "next": {
                    "type": "string",
                    "format": "uri"
                }
            }
        }
    },
    "required": [
        "data"
    ]
}