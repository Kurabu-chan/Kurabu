{
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "**/*.d.ts",
        "**/index.ts",
        "**/interfaces/**",
        "**/*.spec.ts"
    ],
    "reporter": [
        "text-summary",
        "html",
        "lcov"
    ],
    "extension": [
        ".ts"
    ],
    "check-coverage": true,
    "all": true
}
