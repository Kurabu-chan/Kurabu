    "scripts": {
        "format": "prettier --write .",
        "test-setup": "npm install chai mocha ts-node @types/chai @types/mocha",
        "test": "mocha \"tests/**/*.ts\" --config .mocharc.json --timeout 15000",
        "start-dev": "nodemon --config \"./util/nodemon.json\"/",
        "build": "rm -rf ./build/ && tsc",
        "build:windows": "(if exist \".\\build\\\" rmdir /s /q \"./build/\") && tsc",
        "start": "npm run start:linux",
        "start:linux": "TS_NODE_PROJECT=tsconfig.json&& node -r tsconfig-paths/register -r ts-node/register build/start.js",
        "start:windows": "set TS_NODE_PROJECT=tsconfig.json&& node -r tsconfig-paths/register -r ts-node/register build/start.js",
        "db:addmigration": "ts-node -r tsconfig-paths/register ./src/database/createmigration.ts",
        "lint": "eslint \"**/*.ts\"",
        "install:dev": "npm install --only=dev",
        "install:prod": "npm install --only=prod",
        "test:coverage": "tsc && nyc npm run test"
    },
