{
	"extends": "tslint:recommended",
	"rules": {
		"max-line-length": {
			"options": [
				100
			]
		},
		"member-ordering": true,
		"no-consecutive-blank-lines": false,
		"object-literal-sort-keys": true,
		"ordered-imports": true,
		"quotemark": [
			true,
			"double"
		],
		"variable-name": [
			true,
			"allow-leading-underscore"
		]
	}
}